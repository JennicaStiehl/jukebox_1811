<!DOCTYPE html>
<html>
  <head>
    <title>Jukebox</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <% if current_user %>
      <%= link_to 'Log Out', logout_path %>
    <% else %>
      <%= link_to 'Register as a User', new_user_path %>
      <%= link_to 'Log In', login_path %>
    <% end %>

    <%= yield %>
  </body>
</html>
